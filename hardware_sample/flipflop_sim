#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5645fdd04d90 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5645fdd04f20 .scope module, "flipflop_tb" "flipflop_tb" 3 5;
 .timescale -9 -12;
P_0x5645fdd050b0 .param/l "CLK_PER" 1 3 14, +C4<00000000000000000000000000001010>;
v0x5645fdd58710_0 .var "clk_i", 0 0;
v0x5645fdd587b0_0 .var "d_i", 0 0;
v0x5645fdd58880_0 .net "q_o", 0 0, v0x5645fdd58300_0;  1 drivers
v0x5645fdd58980_0 .var "reset_i", 0 0;
E_0x5645fdd42cc0 .event posedge, v0x5645fdd067b0_0;
E_0x5645fdd431b0 .event negedge, v0x5645fdd067b0_0;
S_0x5645fdd42560 .scope module, "dut" "flip_flop" 3 11, 4 1 0, S_0x5645fdd04f20;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk_i";
    .port_info 1 /INPUT 1 "reset_i";
    .port_info 2 /INPUT 1 "d_i";
    .port_info 3 /OUTPUT 1 "q_o";
v0x5645fdd067b0_0 .net "clk_i", 0 0, v0x5645fdd58710_0;  1 drivers
v0x5645fdd06ba0_0 .net "d_i", 0 0, v0x5645fdd587b0_0;  1 drivers
v0x5645fdd58300_0 .var "q_o", 0 0;
v0x5645fdd583a0_0 .net "reset_i", 0 0, v0x5645fdd58980_0;  1 drivers
E_0x5645fdd43450 .event posedge, v0x5645fdd583a0_0, v0x5645fdd067b0_0;
S_0x5645fdd58510 .scope autotask, "tick" "tick" 3 19, 3 19 0, S_0x5645fdd04f20;
 .timescale -9 -12;
TD_flipflop_tb.tick ;
    %wait E_0x5645fdd431b0;
    %wait E_0x5645fdd42cc0;
    %end;
    .scope S_0x5645fdd42560;
T_1 ;
    %wait E_0x5645fdd43450;
    %load/vec4 v0x5645fdd583a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5645fdd58300_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5645fdd06ba0_0;
    %assign/vec4 v0x5645fdd58300_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5645fdd04f20;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5645fdd58710_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5645fdd04f20;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x5645fdd58710_0;
    %inv;
    %store/vec4 v0x5645fdd58710_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5645fdd04f20;
T_4 ;
    %vpi_call/w 3 24 "$dumpfile", "waves.vcd" {0 0 0};
    %vpi_call/w 3 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5645fdd04f20 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5645fdd58980_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5645fdd587b0_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_4.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_4.1, 5;
    %jmp/1 T_4.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %alloc S_0x5645fdd58510;
    %fork TD_flipflop_tb.tick, S_0x5645fdd58510;
    %join;
    %free S_0x5645fdd58510;
    %jmp T_4.0;
T_4.1 ;
    %pop/vec4 1;
    %load/vec4 v0x5645fdd58880_0;
    %cmpi/ne 0, 0, 1;
    %jmp/0xz  T_4.2, 6;
    %vpi_call/w 3 29 "$fatal", 32'sb00000000000000000000000000000001, "q_o not 0 during reset" {0 0 0};
T_4.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5645fdd58980_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5645fdd587b0_0, 0, 1;
    %alloc S_0x5645fdd58510;
    %fork TD_flipflop_tb.tick, S_0x5645fdd58510;
    %join;
    %free S_0x5645fdd58510;
    %load/vec4 v0x5645fdd58880_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz  T_4.4, 6;
    %vpi_call/w 3 34 "$fatal", 32'sb00000000000000000000000000000001, "q_o didn't capture 1 on posedge" {0 0 0};
T_4.4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5645fdd587b0_0, 0, 1;
    %alloc S_0x5645fdd58510;
    %fork TD_flipflop_tb.tick, S_0x5645fdd58510;
    %join;
    %free S_0x5645fdd58510;
    %load/vec4 v0x5645fdd58880_0;
    %cmpi/ne 0, 0, 1;
    %jmp/0xz  T_4.6, 6;
    %vpi_call/w 3 39 "$fatal", 32'sb00000000000000000000000000000001, "q_o didn't capture 0 on posedge" {0 0 0};
T_4.6 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5645fdd587b0_0, 0, 1;
    %wait E_0x5645fdd431b0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5645fdd58980_0, 0, 1;
    %delay 1000, 0;
    %load/vec4 v0x5645fdd58880_0;
    %cmpi/ne 0, 0, 1;
    %jmp/0xz  T_4.8, 6;
    %vpi_call/w 3 45 "$fatal", 32'sb00000000000000000000000000000001, "q_o not cleared by async reset" {0 0 0};
T_4.8 ;
    %wait E_0x5645fdd42cc0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5645fdd58980_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5645fdd587b0_0, 0, 1;
    %alloc S_0x5645fdd58510;
    %fork TD_flipflop_tb.tick, S_0x5645fdd58510;
    %join;
    %free S_0x5645fdd58510;
    %load/vec4 v0x5645fdd58880_0;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz  T_4.10, 6;
    %vpi_call/w 3 51 "$fatal", 32'sb00000000000000000000000000000001, "q_o didn't recover after reset" {0 0 0};
T_4.10 ;
    %vpi_call/w 3 53 "$display", "PASS: flip_flop basic checks OK" {0 0 0};
    %vpi_call/w 3 54 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
